{
    "name": "fullstack-react",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "start": "vite --port 3365",
        "lint": "eslint . --ext .ts,.tsx",
        "format": "npx prettier --write .",
        "typecheck": "tsc --noEmit",
        "lint:fix": "eslint . --ext .ts,.tsx --fix --cache",
        "dev:lint": "concurrently -k -n VITE,ESLINT -c auto \"vite\" \"chokidar ' src/**/*.{ts,tsx}' -c 'eslint {path}'\"",
        "dev:ts": "CHECK_TS=true vite",
        "dev:all": "CHECK_TS=true pnpm dev:lint",
        "build": "vite build",
        "build:typecheck": "tsc -b && vite build",
        "preview": "vite preview"
    },
    "dependencies": {
        "@emotion/cache": "^11.14.0",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@mui/icons-material": "7.3.8",
        "@mui/material": "7.3.8",
        "@reduxjs/toolkit": "^2.11.2",
        "@vercel/analytics": "^1.6.1",
        "ag-grid-community": "35.1.0",
        "ag-grid-react": "35.1.0",
        "axios": "^1.13.5",
        "formik": "^2.4.9",
        "react": "19.2.4",
        "react-dom": "19.2.4",
        "react-intl": "8.1.3",
        "react-redux": "^9.2.0",
        "react-router-dom": "^7.13.0",
        "redux-persist": "^6.0.0",
        "sonner": "^2.0.7",
        "stylis-plugin-rtl": "^2.1.1",
        "yup": "^1.7.1"
    },
    "devDependencies": {
        "@eslint/js": "^10.0.1",
        "@types/node": "25.2.3",
        "@types/react": "19.2.14",
        "@types/react-dom": "19.2.3",
        "@vitejs/plugin-react": "^5.1.4",
        "chokidar-cli": "^3.0.0",
        "csstype": "^3.2.3",
        "eslint": "^10.0.0",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.5.0",
        "globals": "^17.3.0",
        "sass-embedded": "^1.97.3",
        "typescript": "5.9.3",
        "typescript-eslint": "^8.55.0",
        "vite": "^7.3.1",
        "vite-plugin-checker": "^0.12.0"
    },
    "browserslist": [
        "defaults",
        "not IE 11",
        "not op_mini all"
    ],
    "packageManager": "pnpm@10.8.1"
}
